<p id="notice"><%= notice %></p>
<div class="row">
  <div class="col-4">
  <h2>Developer</h2>
  <form method="get" action="/students/checked">
    <div id="student-check">
      <% @students.order(:email).each do |student| %>
        <div class="form-check">
          <input class="form-check-input" name="<%="student-#{student.id}"%>" type="checkbox" value="<%="#{student.id}"%>">
          <label class="form-check-label" for="flexCheckDefault">
            <%="#{student.name}" %>
          </label>
        </div>
      <% end %>
      <%= button_to 'Submit',{}, {class: "btn btn-primary mt-3"} %>
      </div>
    </form>
  </div>
  <div class="col">
    <%= render "output" %>
  </div>
</div>
<br>
<%= link_to 'Add Developer', new_student_path, {class: "btn btn-primary mt-3", style: "color:white;"} %>
